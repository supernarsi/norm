class {{selectorClass}} extends Selector
{
    protected function setModelMapper(): {{mapperClass}}
    {
        return new {{mapperClass}}();
    }

    /**
     * @return {{modelClass}}|Model
     */
    public function createModel(): {{modelClass}}
    {
        return parent::createModel();
    }
}