    protected static string $tableName = '{{tableName}}';
    protected string $modelName = {{modelClassName}}::class;

    public function getTableName(bool $isPartition, string $partitionIdx, bool $prefixMod = false): string
    {
        return $isPartition ? $this->getPartitionTableName($partitionIdx, $prefixMod) : self::$tableName;
    }

