{
    "name": "nineton-nasa/norm",
    "description": "A Basic ORM and DB library",
    "version": "1.0.6",
    "type": "library",
    "homepage": "https://github.com/supernarsi/norm",
    "license": "MIT",
    "authors": [
        {
            "name": "narsi",
            "email": "narsihuang@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4.0",
        "ext-pdo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
